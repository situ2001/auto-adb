    "cli",
    "network"
  ],
  "author": "situ2001 <yongcong2001@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/situ2001/auto-adb.git"
  },
  "homepage": "https://github.com/situ2001/auto-adb#readme",
  "bugs": {
    "url": "https://github.com/situ2001/auto-adb/issues"
  },
  "packageManager": "pnpm@10.16.1",
  "devDependencies": {
    "@types/node": "^24.5.1",
    "tsdown": "^0.15.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "commander": "^14.0.1",
    "execa": "^9.6.0",
    "pino": "^9.10.0",
    "pino-pretty": "^13.1.1",
    "zod": "^4.1.9"
